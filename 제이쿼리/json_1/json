    <script>
        $(function(){
            $.getJSON('book.json',function(data){
                var books=data.book;
                $(books).each(function(i,obj){
                    var title = obj.title;
                    var author = obj.author;
                    var price = obj.price;
                    var txt = "<li>" +title+","+author+","+price+"</li><hr>";
                    $('body').append(txt);
                })
            })
        })
    </script>
</body>
</html>
<!-- 캐시삭제를 해야함 F5해서 변경되지 않음 5분뒤 -->